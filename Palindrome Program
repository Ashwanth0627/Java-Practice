import java.util.*;
import java.util.Scanner;
class Main {
	public static void main(String[]args) {
		Scanner sc = new Scanner(System.in);
		String a=sc.nextLine();
		String rev="";
		for(int i=a.length()-1; i>=0;i--) {
		  rev=rev+a.charAt(i);
		}
		System.out.println(a.equals(rev)? "Palindrome ":"Not Palindrome");

	}
}
